#!/bin/bash
# Script: server_trigger
# Description: Script for handling all server events

flag=$(echo $line | cut -d' ' -f2)

case $flag in
	311)	# whois response containing username and hostmask
		 . bin/server_triggers/311
		return
		;;
	353)	# Userlist of some channel
		. bin/server_triggers/353
		return
		;;
esac

